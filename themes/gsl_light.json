{
    "name": "GSL Light",
	"type": "light",
	"$schema": "vscode://schemas/color-theme",
	"include": "./light_plus.json",
	"tokenColors": [
		{
			"scope": "comment.gsl.scan.index.ten",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "keyword.gsl",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": "keyword.gsl.control",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"scope": "keyword.gsl.dangerous",
			"settings": {
				"foreground": "#C64844",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "keyword.gsl.stop",
			"settings": {
				"foreground": "#AE66CF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "keyword.gsl.stack",
			"settings": {
				"foreground": "#AE66CF"
			}
		},
		{
			"scope": "string.quoted.double.gsl",
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"scope": "token.gsl",
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"scope": "variable.gsl",
			"settings": {
				"foreground": "#267f99"
			}
		},
		{
			"scope": "variable.gsl.system",
			"settings": {
				"foreground": "#267f99"
			}
		},
		{
			"scope": "variable.gsl.node",
			"settings": {
				"foreground": "#CC8300",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "variable.gsl.field",
			"settings": {
				"foreground": "#CC8300"
			}
		}
    ]
}