{
    "version": "0.2",
    "files": [
        "*.gsl"
    ],
    "enableFiletypes": [
        "gsl"
    ],
    "minWordLength": 3,
    "dictionaries": [
        "GemStone"
    ],
    "dictionaryDefinitions": [
        {
            "name": "GemStone",
            "path": "./GemStoneDictionary.txt",
            "addWords": true
        }
    ],
    "includeRegExpList": [
        "\"(.*?)\""
    ],
    "ignoreRegExpList": [
        "callmatch.*\"(.*?)\"",
        "set \\$MATCH to \"(.*?)\"",
        "\\:\\s+\"(.*?)\""
    ]
}